	PROCEDURE GrdListSucur.Column3.cmdModificar.Click
		CCurso = ThisForm.cCurTempo
		ThisForm.cValSucur= ALLTRIM(&CCurso..cdg_loc )
		DO FORM frm_loca WITH ThisForm.cValSucur, "1"
		
		
		ThisForm.cmdBuscar.Click()
		CCurso = ThisForm.cCurTempo
		SELECT (CCurso )
		LOCATE FOR &CCurso..cdg_loc = ThisForm.cValSucur
		
	ENDPROC

	PROCEDURE GrdListSucur.Column4.cmEliminar.Click
		CCurso = Thisform.cCurTempo
		Thisform.cValSucur = Alltrim(&CCurso..cdg_loc )
		cCond = "cdg_loc  ='" +Thisform.cValSucur+ "'"
		CvarCur = Thisform.ojbLocal.Select("", "",  cCond , "1")
		
		Thisform.ojbLocal.llenar(CvarCur )
		USE IN (CvarCur) 
		nVal =Messagebox("Está seguro de eliminar a " + Alltrim(&CCurso..des_loc)+" ?",4+32+256,"Eliminar registro")
		If  nVal= 6
		
			If Thisform.ojbLocal.eliminar()
				Messagebox("registro Eliminado ")
			Else
				Messagebox(ObjTablas.mensaje)
			Endif
		
		Endif
		
		
		Thisform.cmdBuscar.Click()
		ThisForm.txtBuscar.SetFocus()
	ENDPROC

	PROCEDURE GrdListSucur.Column5.cmdHorario.Click
		CCurso = ThisForm.cCurTempo
		ThisForm.cValSucur= ALLTRIM(&CCurso..cdg_loc )
		DO FORM frm_horaloca WITH ThisForm.cValSucur && , "1"
		
		
		ThisForm.cmdBuscar.Click()
		CCurso = ThisForm.cCurTempo
		SELECT (CCurso )
		LOCATE FOR &CCurso..cdg_loc = ThisForm.cValSucur
		
	ENDPROC

