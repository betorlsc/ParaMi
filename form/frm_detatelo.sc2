		SELECT (CCurso )
		LOCATE FOR &CCurso..cdg_plan = ThisForm.cValEmple
		
	ENDPROC

	PROCEDURE GrdListEmple.Column6.cmEliminar.Click
		CCurso = Thisform.cCurTempo
		Thisform.cValEmple = Alltrim(&CCurso..cdg_plan )
		cCond = "cdg_plan  ='" +Thisform.cValEmple+ "'"
		CvarCur = Thisform.ojbEmple.Select("", "",  cCond , "1")
		
		Thisform.ojbEmple.llenar(CvarCur )
		USE IN (CvarCur) 
		nVal =Messagebox("Está seguro de eliminar a " + Alltrim(&CCurso..des_emple )+" ?",4+32+256,"Eliminar registro")
		If  nVal= 6
		
			If Thisform.ojbEmple.eliminar()
				Messagebox("registro Eliminado ")
			Else
				Messagebox(ObjTablas.mensaje)
			Endif
		
		Endif
		
		
		Thisform.cmdBuscar.Click()
		ThisForm.txtbuscar.SetFocus()
	ENDPROC

