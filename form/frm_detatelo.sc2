	PROCEDURE Destroy
		CCurso = ThisForm.cCurTempo 
		
		IF USED(CCurso)
		USE IN (CCurso)
		
		ENDIF
	ENDPROC

	PROCEDURE Init
		Thisform.AddObject("ojbEmple", "M_Emple")
		
		ThisForm.AddProperty("cCurTempo","")
		ThisForm.AddProperty("cValEmple","")
		Thisform.GrdListEmple.RecordSource= ""
		
		
		cone.conecta()
		cCampos= "cdg_plan, allt(nombres)+ ' ' + allt(apellidos) des_emple ,  dni, fec_ingre"
		cCond = ""
		CCurso = Thisform.ojbEmple.Select("", cCampos,  cCond , "100")
		ThisForm.cCurTempo = CCurso 
		cone.desconecta() 
		Select (CCurso )
		GO TOP 
		INDEX on des_emple TAG id
		Thisform.GrdListEmple.RecordSource= (CCurso )
		Thisform.Refresh()
		
		
	ENDPROC

	PROCEDURE cmdBuscar.Click
		
		ThisForm.GrdListEmple.RecordSource= ""
		
		cTextoBusca= Alltrim(Thisform.txtBuscar.Value)
		cone.conecta()
		cCampos= "cdg_plan, allt(nombres)+ ' ' + allt(apellidos) des_emple ,  dni, fec_ingre"
		cCond = ""
		cValor = Chrtran(cTextoBusca, " ", "%")
		
		cCursorTmp  = ThisForm.cCurTempo 
		
		IF USED(cCursorTmp)
		USE IN (cCursorTmp )
		ENDIF
		
		If Empty(cTextoBusca)
			CCurso = Thisform.ojbEmple.Select("", cCampos,  cCond , "100")
		
		else
			cCond = "allt(nombres)+ ' ' + allt(apellidos) like '%"+cValor +"%' or  ALLTRIM(dni)  like '%"+cValor +"%' or  ALLTRIM(cdg_plan)  like '%"+cValor +"%'"
			CCurso = Thisform.ojbEmple.Select("", cCampos,  cCond , "100")
		ENDIF
		 ThisForm.cCurTempo   = CCurso 
		cone.desconecta()
		SELECT (CCurso )
		GO TOP 
		INDEX on des_emple  TAG sd
		ThisForm.GrdListEmple.RecordSource= (CCurso )
		ThisForm.Refresh()
		
	ENDPROC

