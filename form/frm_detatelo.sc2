	PROCEDURE GrdListEmple.Column5.cmdModificar.Click
		CCurso = ThisForm.cCurTempo
		ThisForm.cValEmple= ALLTRIM(&CCurso..cdg_plan )
		DO FORM frm_Emple WITH ThisForm.cValEmple, "1"
		ThisForm.cmdBuscar.Click()
		
