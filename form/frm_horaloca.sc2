	ENDPROC

	PROCEDURE Init
		Parameters cCodLoc
		
		Thisform.AddObject("ojbHorSuc", "M_Hrloca")
		Thisform.AddObject("ojbEmpre", "M_Empre")
		Thisform.AddObject("ojbLoc", "d_local")
		Thisform.AddProperty("cOpc", "cOpc")
		
		CCurso = Thisform.ojbLoc.Select("" , "",  "cdg_loc ='"+cCodLoc+"'" , "1")
		ThisForm.ojbHorSuc.cCdgLoc = cCodLoc
		
		ThisForm.txtCdg_loc.Value = Alltrim(&CCurso..des_loc)
		
		USE IN (CCurso )
		Thisform.Refresh()
		
	ENDPROC

	PROCEDURE limpiarctrl
		ThisForm.txtLVManaIngre.Value = ""
		ThisForm.txtLVManaSalid.Value = ""
		ThisForm.txtLVTardeIngre.Value = ""
		ThisForm.txtLVTardeSalid.Value = ""
		ThisForm.txtSManaIngre.Value = ""
		ThisForm.txtSManaSalid.Value = ""
		ThisForm.txtSTardeIngre.Value = ""
		ThisForm.txtSTardeSalid.Value = ""
		
	ENDPROC

	PROCEDURE validar
		cValor =""
		DO CASE
		CASE EMPTY(ThisForm.txtCdg_emp.Value)
		
		cValor = "Debe seleccionar la empresa"
		
		CASE EMPTY(ThisForm.txtCdg_loc.Value)
		
		cValor = "Debe seleccionar el local"
		
		OTHERWISE
		
		ENDCASE
		
		RETURN cValor 
	ENDPROC

	PROCEDURE Cmdacepta1.Click
		cMensajeVal = Thisform.validar()
		If Empty(cMensajeVal )
			Thisform.actulizahora(Thisform.txtLVManaIngre.Value, "01", "01", "01")
			Thisform.actulizahora(Thisform.txtLVManaSalid.Value, "01", "01", "02")
			Thisform.actulizahora(Thisform.txtLVTardeIngre.Value, "01", "02", "01")
			Thisform.actulizahora(Thisform.txtLVTardeSalid.Value, "01", "02", "02")
			Thisform.actulizahora(Thisform.txtSManaIngre.Value, "02", "01", "01")
			Thisform.actulizahora(Thisform.txtSManaSalid.Value, "02", "01", "02")
			Thisform.actulizahora(Thisform.txtSTardeIngre.Value, "02", "02", "01")
			Thisform.actulizahora(Thisform.txtSTardeSalid.Value, "02", "02", "02")
			Thisform.Release()
		Else
			Messagebox(cMensajeVal,0+48 , "Datos Incompletos")
		Endif
	ENDPROC

	PROCEDURE cmdBusEmpresa.Click
		*!*	oControl = ThisForm.txtDes_dpt&& control que recibe el resultado de la busqueda 
		*!*	oControl.GotFocus
		idcamp = "cdg_emp" && valor id de la tabla 
		cCampoFiltro = "des_emp" && valor campo a buscar en la tabla 
		oTablaAux = Thisform.ojbEmpre && Objeto tabla auxiliar 
		cCampos = cCampoFiltro +", "+idcamp
		cAdi =''
		oObjReporno = NEWOBJECT("Custom")
		OBJfrm_bsopc = CREATEOBJECT("frmbuscatab",oTablaAux , cCampos, cCampoFiltro,  cAdi, oObjReporno, ""  )
		OBJfrm_bsopc.show()
		nValor = oObjReporno.cdg && verifivar si es cadena o numero 
		RELEASE oObjReporno
		Thisform.ojbHorSuc.cCdgEmp = nValor  &&  porpiedad id del ojeto 
		
		
		CCurso = Thisform.ojbEmpre.Select("" , "",  idcamp +" ='"+nValor  +"'" , "1")
		
		Thisform.txtCdg_emp.Value = Alltrim(&CCurso..des_emp)
		
		Use In &CCurso
		cValor =ThisForm.ojbHorSuc.cCdgLoc
		xValor = Thisform.ojbHorSuc.cCdgEmp
		CCurso = Thisform.ojbHorSuc.Select("" , "",  " cdg_emp ='"+xValor +"' and cdg_loc ='"+cValor  +"'" , "8")
		SELECT (CCurso )
		INDEX on  tipo+cdg_tur+cdg_mot TAG id 
		ThisForm.limpiarctrl()
		
		ThisForm.txtLVManaIngre.Value = IIF(SEEK("010101", CCurso ),&CCurso..hora, "00:00" )
		ThisForm.txtLVManaSalid.Value = IIF(SEEK("010102", CCurso ),&CCurso..hora, "00:00" )
		ThisForm.txtLVTardeIngre.Value = IIF(SEEK("010201", CCurso ),&CCurso..hora, "00:00" )
		ThisForm.txtLVTardeSalid.Value = IIF(SEEK("010202", CCurso ),&CCurso..hora, "00:00" )
		ThisForm.txtSManaIngre.Value = IIF(SEEK("020101", CCurso ),&CCurso..hora, "00:00" )
		ThisForm.txtSManaSalid.Value = IIF(SEEK("020102", CCurso ),&CCurso..hora, "00:00" )
		ThisForm.txtSTardeIngre.Value = IIF(SEEK("020201", CCurso ),&CCurso..hora, "00:00" )
		ThisForm.txtSTardeSalid.Value = IIF(SEEK("020202", CCurso ),&CCurso..hora, "00:00" )
		
		Use In &CCurso
		
		
		ThisForm.Refresh()
	ENDPROC

