	PROCEDURE actulizahora
		Parameters cValControl,  cTipo,  cCdgTur, cCdgMot
		Thisform.ojbHorSuc.cTipo = cTipo
		Thisform.ojbHorSuc.cCdgTur = cCdgTur
		Thisform.ojbHorSuc.cCdgMot = cCdgMot
		Thisform.ojbHorSuc.cHora = cValControl
		cValor =Thisform.ojbHorSuc.cCdgLoc
		xValor = Thisform.ojbHorSuc.cCdgEmp
		If cValControl<> "00:00"
			cCur = Thisform.ojbHorSuc.Select("" , "",  " CDG_EMP = '" + xValor  + "' and  CDG_LOC = '" + cValor  + "' and  CDG_MOT = '" + cCdgMot + "' and  CDG_TUR = '" + cCdgTur + "' and  TIPO = '" + cTipo + "'" , "1")
			If Reccount(cCur)>0
				Thisform.cOpc= "1"
			Else
				Thisform.cOpc= "0"
			Endif
			Use In (cCur)
			If Thisform.cOpc= "0"
				If Thisform.ojbHorSuc.ActivaReg()= "0"
					Thisform.ojbHorSuc.insertar()
				Else
					Thisform.ojbHorSuc.modificar()
				Endif
			Else
				Thisform.ojbHorSuc.modificar()
			Endif
		Else
			Thisform.ojbHorSuc.Eliminar()
		Endif
	ENDPROC

